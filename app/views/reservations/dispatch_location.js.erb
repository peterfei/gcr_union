<%if @o%>
  $(".order_<%=@reservation.id%>").html("<%=j(render :partial=>'list',:locals=>{:reservation=>@reservation})%>");
  $("#myModal").modal('hide');

<%-else%> 
var _html = ("<%=j(render 'reservations/dispatch_location_form')%>");
$('#myModal').html(_html);
$("#myModal").modal('show');
$('#myModal').on('shown', function (e) {
  $(_html).last().appendTo('#myModal');
  $('#pickup_city').pickup_city_select2();
  $('#pickup_district').pickup_district_select2();
  $('#location_id').location_select2();
})
<%-end%>
