<%-unless $o%>
var _html =("<%=j(render 'reservations/dispatch_car_form')%>");
$('#myModal').html(_html);
$("#myModal").modal('show');
$('#myModal').on('shown', function (e) {
  $(_html).last().appendTo('#myModal');
  $('#location_id').location_select2();
  $('#car_id').car_info_select2();
  $('#driver_id').driver_info_select2();
})
<%else%>
  $(".order_<%=@reservation.id%>").html("<%=j(render :partial=>'list',:locals=>{:reservation=>@reservation})%>");
$("#myModal").modal('hide');
<%-end%>
