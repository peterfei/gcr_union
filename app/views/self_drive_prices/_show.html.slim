p#notice= notice
table.table.table-hover
  tr
    td 车辆品牌
    td=@self_drive_prices.car_model
  tr
    td== link_to '周内价格', @self_drive_prices.edit_path, remote: true
    td= @self_drive_prices.weekday_prices
  tr
    td== link_to '周末价格', @self_drive_prices.edit_path, remote: true
    td=@self_drive_prices.weekend_prices
  -@self_drive_prices.range.each do |range,prices|
    tr
      td== link_to range, @self_drive_prices.edit_path, remote: true
      td=prices
